{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "OrigamiKings Robotics Armor Mod",
	"description": "A mod that adds armor styled as Transformers",
	"authors": [
		"OrigamiKing3612",
		"ElihueD12"
	],
	"contact": {
		"homepage": "https://mods.origamiking.net",
		"sources": "https://github.com/OrigamiKing3612/OrigamiKings-Robotics-Mod"
	},
	"license": "Apache-2.0",
	"icon": "assets/orm/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.origamiking.mcmods.orm.OrmMain"
		],
		"fabric-datagen": [
			"net.origamiking.mcmods.orm.datagen.ModDataGenerator"
		],
		"client": [
			"net.origamiking.mcmods.orm.OrmClient"
		],
		"rei_client": [
          "net.origamiking.mcmods.orm.compact.rei.OrmREIClientPlugin"
		],
		"emi": [
		  "net.origamiking.mcmods.orm.compact.emi.OrmEMIClientPlugin"
		],
		"modmenu": [
			"net.origamiking.mcmods.orm.compact.modmenu.OrmModMenuCompact"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json"
	],
	"jars": [
		{
			"file": "jars/Origamikings-API-${origamikings_api}.jar"
		}
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": ">=${minecraft_version}",
		"java": ">=17",
		"fabric-api": ">=${fabric_version}",
		"geckolib": ">=${geckolib_version}",
		"origamikings-api": ">=${origamikings_api}"
	},
	"suggests": {
		"rei": "*",
		"emi": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.website": "https://mods.origamiking.net",
				"modmenu.modrinth": "https://modrinth.com/mod/origamikings-robotics-armor-mod"
			},
			"badges": [],
			"update_checker": true
		}
	}
}
